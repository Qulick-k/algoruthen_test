      },
      "source": [
        "d(S,T) = mid{ 1+d(A,T) , 2+d(B,T) , 5+d(C,T) }\r\n",
        "d(A,T)=min{ 4+d(D,T) }             d(A,T)=6\r\n",
        "d(B,T)=min{ 11+d(D,T) , 13 , 16+d(F,T) } d(B,T)=12\r\n",
        "d(C,T)=min{ 2+d(F,T) }              d(C,T)=9\r\n",
        "d(S,T) = mid{ 1+d(A,T) , 2+d(B,T) , 5+d(C,T) }\r\n",
        "     =27 不是解\r\n",
        "\r\n",
        "d(S,A) = 1\r\n",
        "d(S,B) = 2\r\n",
        "d(S,C) = 5\r\n",
        "\r\n",
        "d(S,D)=min{d(S,A)+d(A,D)}\r\n",
        "     =min{1+4}=5\r\n",
        "d(S,B)=2\r\n",
        "d(S,F)=min{d(S,C),d(C,F)}=7\r\n",
        "d(S,T)=min{ d(S,D)+d(D,T) , d(S,B)+d(B,T) , d(S,F)+d(F,T) }\r\n",
        "     =min{6,14,9}\r\n",
        "     =6  最佳解\r\n"
      ]
    }
  ]
}